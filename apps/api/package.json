{
  "name": "maxflow-shift-optimizer",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "setup": "prisma generate && prisma migrate dev",
    "start": "node index.js",
    "dev": "node --watch index.js",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:studio:test": "dotenv -e .env.test -- prisma studio",
    "test": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed && dotenv -e .env.test -- vitest run --pool=forks --fileParallelism=false --max-concurrency=1",
    "test:ci": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed && dotenv -e .env.test -- vitest run --pool=forks --fileParallelism=false --max-concurrency=1 --reporter=dot",
    "db:scenario:feasible": "prisma migrate reset --force --skip-seed && node prisma/scenarios/feasible.js",
    "db:scenario:infeasible": "prisma migrate reset --force --skip-seed && node prisma/scenarios/infeasible.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "Optimizador de cobertura de personal para guardias hospitalarias usando algoritmos de flujo.",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ics": "^3.8.1",
    "jsonwebtoken": "^9.0.3",
    "prisma": "^5.22.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6",
    "@maxflow/shared": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
